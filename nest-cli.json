{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "base-entity": {
      "type": "library",
      "root": "libs/base-entity",
      "entryFile": "index",
      "sourceRoot": "libs/base-entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base-entity/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "mailer": {
      "type": "library",
      "root": "libs/mailer",
      "entryFile": "index",
      "sourceRoot": "libs/mailer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mailer/tsconfig.lib.json"
      }
    },
    "media": {
      "type": "library",
      "root": "libs/media",
      "entryFile": "index",
      "sourceRoot": "libs/media/src",
      "compilerOptions": {
        "tsConfigPath": "libs/media/tsconfig.lib.json"
      }
    },
    "pagination": {
      "type": "library",
      "root": "libs/pagination",
      "entryFile": "index",
      "sourceRoot": "libs/pagination/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pagination/tsconfig.lib.json"
      }
    },
    "upload": {
      "type": "library",
      "root": "libs/upload",
      "entryFile": "index",
      "sourceRoot": "libs/upload/src",
      "compilerOptions": {
        "tsConfigPath": "libs/upload/tsconfig.lib.json"
      }
    }
  }
}